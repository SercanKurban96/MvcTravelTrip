@using MvcTravelTrip.Models.Classes
@model List<About>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_TestLayout.cshtml";
}

<div class="content">
    <div class="wrap">
        <div class="about">
            <h3 align="center">Hakkımızda</h3>
            @foreach (var item in Model)
            {
                <img src="@item.PhotoUrl" alt="" width="1000" height="400" />
                <p>@item.Description</p>
            }
        </div>
    </div>
</div>